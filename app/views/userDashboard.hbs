<!-- Available Surveys -->
<div class = "container fluid">
  <div class = "row-margin">
  </div>
  <div class = "row">
    <div class = "col-md-1">
    </div>
    <div class = "col-md-6">
      <div class = "main main-container">
        <h2>Available Surveys</h2>
        <hr>
        <a href="search" style="color:black;">search</a>
      <!--  <div class="col">-->
          <a href="#surveyModal1" class="col portfolio-link" data-toggle="modal">
            Demographic Survey
          </a>
      <!--  </div>
        <div class = "col">-->
          <a href="#surveyModal2" class="col portfolio-link" data-toggle="modal">
            Interests Survey
          </a>
      <!--  </div>-->
      </div>
    </div>

    <div class = "col-md-4">
      <div class = "main main-container">
        <h2>Account Balance</h2>
        <hr>
        <h2><img src = "assets/images/wallet.png" class= "wallet">$0.00</h2>
      </div>
      <div class = "main main-container">
        <h2>Your InfoCurrency Profile</h2>
        <hr>
<!--{{#if userData.age}} <h4>Age: <span>{{userData.age}}</span></h4>{{/if}}
        {{#if userData.status}} <h4>Status <span>{{userData.status}}</span></h4> {{else}} {{/if}}
        {{#if userData.income}} <h4>Income <span>{{userData.income}}</span></h4> {{else}} {{/if}}
